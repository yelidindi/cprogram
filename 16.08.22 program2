#include <stdio.h>

int main() {
   int arr[5];
   int i;
   printf("enter elements into array\n");
   for(i=0;i<5;i++)
   scanf("%d",&arr[i]);
   int j;
   int flag;
   printf("the primes numbers of array are\n");
   for(i=0;i<5;i++)
   {
       flag=0;
       for(j=2;j<arr[i];j++)
       {
           if(arr[i]%j==0)
           {
               flag=1;
               break;
            }
       }
       if(flag==0)
       printf("%d\t",arr[i]);
   }
   return 0;
}
